<core:FragmentDefinition xmlns="sap.m"
	xmlns:f="sap.ui.layout.form"
xmlns:core="sap.ui.core">
<Dialog id="_IDGenDialog1" title="Product" >
<VBox id="_IDGenVBox1" class="sapUiSmallMargin">
<f:SimpleForm id="SimpleFormChange354"
			editable="true"
			layout="ResponsiveGridLayout"
			labelSpanXL="5"
			labelSpanL="5"
			labelSpanM="5"
			labelSpanS="12"
			adjustLabelSpan="false"
			emptySpanXL="4"
			emptySpanL="4"
			emptySpanM="4"
			emptySpanS="4"
			columnsXL="5"
			columnsL="5"
			columnsM="5"
			singleContainerFullSize="false" >
            <f:content>
             <Label id="_IDGenLabel1" text="{i18n>ProductName}"/>
                    <Input id="_IDGenInput1" value="{prdctmodel>/ProductName}"/>
                     <Label id="_IDGenLabel2" text="{i18n>SupplierID}"/>
                    <Input id="_IDGenInput2" value="{prdctmodel>/SupplierID}"/>
                     <Label id="_IDGenLabel3" text="{i18n>CategoryID}"/>
                    <Input id="_IDGenInput3" value="{prdctmodel>/CategoryID}"/>
                     <Label id="_IDGenLabel4" text="{i18n>QuantityPerUnit}"/>
                    <Input id="_IDGenInput4" value="{prdctmodel>/QuantityPerUnit}"/>
                     <Label id="_IDGenLabel5" text="{i18n>UnitPrice}"/>
                    <Input id="_IDGenInput5" value="{prdctmodel>/UnitPrice}"/>
            		</f:content>
		</f:SimpleForm>
</VBox>
<buttons>          
            <Button id="_IDGenButton4" text="Save"  press="onSave"  width="auto" class="sapUiTinyMargin"/>
            <Button id="_IDGenButton5" text="Cancel"  press="onCancel"  width="auto" class="sapUiTinyMargin"/>
          </buttons>

</Dialog>
    </core:FragmentDefinition>