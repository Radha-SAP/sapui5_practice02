<mvc:View controllerName="ar.com.travelbooking.controller.Travel"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
        <Panel id="_IDGenPanel1">
         <Table id="_IDGenTable1" items="{/Travel}" updateFinished=".onUpdateFinished" mode="SingleSelectLeft">
		<headerToolbar>
			<Toolbar id="_IDGenToolbar1">
				<ToolbarSpacer id="_IDGenToolbarSpacer1"/>
				<Button id="_IDGenButton1" text="Create"  press="onCreate"/>
                <Button id="_IDGenButton2" text="Update"  press="onUpdate"/>
				<Button id="_IDGenButton3" text="Delete"  press="onDelete"/>
            </Toolbar>
		</headerToolbar>
                    <columns>
                        <Column id="_IDGenColumn1">
                            <Text id="_IDGenText1" text="{i18n>agencyId}" />
                        </Column>
                        <Column id="_IDGenColumn2">
                            <Text id="_IDGenText2" text="{i18n>bookingfee}" />
                        </Column>
                        <Column id="_IDGenColumn3">
                            <Text id="_IDGenText_3" text="{i18n>currencycode}" />
                        </Column>
                        <Column id="_IDGenColumn4">
                            <Text id="_IDGenText4" text="{i18n>customerid}" />
                        </Column>
                        <Column id="_IDGenColumn5">
                            <Text id="_IDGenText5" text="{i18n>description}" />
                        </Column>
                      </columns>
                      <ColumnListItem id="_IDGenColumnListItem1" >
                      <cells>
                      <Text id="_IDGenText6" text="{AgencyID}" >
                      </Text>
                      </cells>
                      <cells>
                      <Text id="_IDGenText7"  text="{BookingFee}">
                      </Text>
                      </cells>
                      <cells>
                      <Text id="_IDGenText8" text="{CurrencyCode}">
                      </Text>
                      </cells>
                      <cells>
                      <Text id="_IDGenText9" text="{CustomerID}">
                      </Text>
                      </cells>
                      <cells>
                      <Text id="_IDGenText10" text="{Description}">
                      </Text>
                      </cells>
                      </ColumnListItem>
                     </Table>
                 </Panel>   
        </content>
    </Page>
</mvc:View>
